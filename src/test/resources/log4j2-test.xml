<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="TRACE" monitorInterval="30">

    <Appenders>
        <Console name="STDOUT">
            <PatternLayout pattern="%d %p [%c] - %m%n"/>
        </Console>

    </Appenders>

    <Loggers>
        <Root level="ERROR">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>

</Configuration>
